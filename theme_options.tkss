}

LabelFrame{
    foreground: "#ffffff";
    background: "#e83225";
    borderwidth: 1;
    labelanchor: "nw";/*options available: nw, n, ne, wn, w, ws, sw, s, se, en, e, es*/
    cursor: "clock";
    width: 30;
    height: 14;
    highlightbackground: "#ffffff";
    highlightcolor: "#000000";
    highlightthickness: 1;
    relief: flat;
    padx: 1;
    pady: 2;
}

PanedWindow{
    background: "#e83225";
    borderwidth: 1;
    cursor: "clock";
    width: 30;
    height: 14;
    relief: "flat";
    handlepad: 3; /*distance between slash and handlepad*/
    handlesize: 3;
    orient: vertical;
    sashpad: 0;
    sashrelief: "flat";
    sashwidth: 5;
    showhandle: true;/*bool value*/
}

Spinbox{
    foreground: "#ffffff";
